{
  "name": "tinypng",
  "displayName": "TinyPNG",
  "description": "Use TinyPNG from within VSCode",
  "version": "0.0.1",
  "publisher": "andi1984",
  "engines": {
    "vscode": "^1.18.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.compressFile"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.compressFile",
        "title": "Compress Image"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceLangId == tinypng_file",
          "command": "extension.compressFile",
          "group": "1_modification"
        }
      ]
    },
    "languages": [
      {
        "id": "tinypng_file",
        "extensions": [
          ".png",
          ".jpg",
          ".jpeg"
        ]
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.6",
    "eslint": "^4.6.1",
    "@types/node": "^7.0.43",
    "@types/mocha": "^2.2.42"
  }
}
